{
  "name": "@ngx-material-dashboard/ngx-material-dashboard",
  "version": "0.7.2-beta.0",
  "author": "Jonathan Phillips (https://github.com/jphillips03)",
  "keywords": [
    "angular",
    "angular material dashboard",
    "dashboard",
    "json",
    "json api",
    "ngx",
    "testing"
  ],
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:playground": "ng serve playground",
    "start:example": "concurrently --kill-others --raw \"ng serve example-client\" \"npm run start:example-server\"",
    "start:example-server": "cd projects && cd example && cd server && npm start",
    "build": "ng build base-json && ng build testing && ng build json-api && ng build json && ng build widgets",
    "build:example": "concurrently \"npm run build\" \"npm run install:example-server\"",
    "install:example-server": "cd projects && cd example && cd server && npm install",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --no-watch",
    "test:headless": "ng test --browsers=ChromeHeadlessNoSandbox --no-watch",
    "test:coverage": "ng test --browsers=ChromeHeadlessNoSandbox --no-watch --code-coverage",
    "playground": "angular-playground",
    "documentation:start": "ng serve documentation",
    "documentation:api": "typedoc --json ./docs/docs.json",
    "documentation:parse-api-json": "npx ts-node ./tools/src/main.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier:format": "prettier --config .prettierrc 'projects/**/*.ts' --write"
  },
  "dependencies": {
    "@angular/animations": "~13.3.6",
    "@angular/cdk": "^13.3.2",
    "@angular/common": "~13.3.6",
    "@angular/compiler": "~13.3.6",
    "@angular/core": "~13.3.6",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "~13.3.6",
    "@angular/material": "^13.3.2",
    "@angular/platform-browser": "~13.3.6",
    "@angular/platform-browser-dynamic": "~13.3.6",
    "@angular/router": "~13.3.6",
    "@fortawesome/angular-fontawesome": "^0.10.2",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "clipboard": "^2.0.11",
    "gumshoejs": "^5.1.2",
    "ngx-markdown": "^13.1.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.5",
    "@angular/cli": "~13.3.5",
    "@angular/compiler-cli": "~13.3.6",
    "@types/jasmine": "~3.10.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "angular-playground": "^9.0.1",
    "colorette": "^2.0.19",
    "concurrently": "^8.2.1",
    "date-fns": "^2.28.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "handlebars": "^4.7.7",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lodash-es": "^4.17.21",
    "ng-mocks": "^13.3.0",
    "ng-packagr": "^13.0.0",
    "prettier": "^2.8.1",
    "replace-in-file": "^6.3.5",
    "ts-node": "^10.8.1",
    "typedoc": "^0.23.23",
    "typedoc-json-parser": "^7.1.0",
    "typescript": "^4.6.4"
  }
}
